<?xml version="1.0"?>
<configuration>
	<appSettings>
		<add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
	</appSettings>
	<connectionStrings>
		<add name="SEZAC" connectionString="server=localhost;user id=root;password=holi;database=sezac;pooling=false" providerName="MySql.Data.MySqlClient"/>
	</connectionStrings>
	<system.web>
		<authentication mode="Forms">
			<forms name=".sezac" path="/" loginUrl="Login.aspx" slidingExpiration="true" timeout="20"/>
		</authentication>
		<authorization>
			<deny users="?"/>
		</authorization>
		<compilation debug="true" targetFramework="4.5"/>
		<customErrors mode="Off"/>
		<httpRuntime targetFramework="4.5"/>
		<globalization culture="ES-MX" uiCulture="ES-MX"/>
		<sessionState mode="InProc" timeout="20"/>
	</system.web>
</configuration>